{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "dev": "GATSBY_CONCURRENT_DOWNLOAD=1 gatsby develop",
    "lint": "eslint .",
    "validate": "npm-run-all --parallel format lint build"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.eslintignore"
    }
  },
  "dependencies": {
    "@popperjs/core": "^2.4.4",
    "babel-plugin-styled-components": "^1.10.7",
    "bootstrap": "^4.5.2",
    "dotenv": "^8.2.0",
    "eslint-plugin-import": "^2.22.0",
    "gatsby": "^2.20.25",
    "gatsby-background-image": "^1.1.1",
    "gatsby-cli": "^2.12.89",
    "gatsby-env-variables": "^2.0.0",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-cookiehub-banner": "^0.1.0",
    "gatsby-plugin-feed": "^2.5.11",
    "gatsby-plugin-gdpr-tracking": "^1.1.0",
    "gatsby-plugin-google-analytics": "^2.3.3",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sass": "^2.2.3",
    "gatsby-plugin-segment-js": "^3.3.0",
    "gatsby-plugin-sharp": "^2.5.6",
    "gatsby-plugin-sitemap": "^2.3.5",
    "gatsby-plugin-styled-components": "^3.2.3",
    "gatsby-plugin-webfonts": "^1.1.3",
    "gatsby-source-filesystem": "^2.2.4",
    "gatsby-source-strapi": "0.0.12",
    "gatsby-transformer-remark": "^2.8.7",
    "gatsby-transformer-sharp": "^2.4.6",
    "jquery": "^3.5.1",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.12.0",
    "react-dropdown": "^1.8.0",
    "react-helmet": "^6.0.0",
    "react-icons": "^3.10.0",
    "react-js-pagination": "^3.0.3",
    "react-markdown": "^4.3.1",
    "react-moment": "^0.9.7",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "babel-preset-gatsby": "^0.3.3",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.1.2",
    "jest": "^25.4.0",
    "jest-runner-eslint": "^0.10.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.1.0",
    "react-test-renderer": "^16.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
